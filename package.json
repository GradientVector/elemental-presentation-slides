{
  "name": "elemental-presentation-slides",
  "version": "0.0.1",
  "description": "Elemental - Customizable User Interfaces using Dynamic Components in Angular",
  "homepage": "https://gradientvector.github.io/elemental-presentation-slides",
  "scripts": {
    "start": "concurrently --kill-others \"npm run watch\" \"npm run dev\"",
    "start-no-remote": "concurrently --kill-others \"npm run watch-no-remote\" \"npm run dev\"",
    "dev": "lite-server",
    "watch": "webpack --watch --mode development",
    "watch-no-remote": "webpack --watch --mode development --env.no-remote=true",
    "build": "webpack --mode production",
    "local": "concurrently --kill-others \"webpack --watch --mode development --env.local=true\" \"npm run dev\""
  },
  "author": {
    "name": "John Harvey",
    "email": "thegradientvector@gmail.com",
    "web": "https://thegradientvector.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/GradientVector/elemental-presentation-slides.git"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "deckdeckgo-webpack-plugins": "^1.0.0-alpha.6",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "lite-server": "^2.5.3",
    "progress-bar-webpack-plugin": "^1.12.1",
    "style-loader": "^1.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "license": "MIT",
  "dependencies": {
    "@deckdeckgo/charts": "^1.0.0-alpha.8",
    "@deckdeckgo/core": "^1.0.0-beta.45-1",
    "@deckdeckgo/highlight-code": "^1.0.0-alpha.17-3",
    "@deckdeckgo/lazy-img": "^1.0.0-alpha.11",
    "@deckdeckgo/qrcode": "^1.0.0-alpha.9",
    "@deckdeckgo/remote": "^1.0.0-alpha.11",
    "@ionic/core": "^4.7.1",
    "@webcomponents/custom-elements": "^1.2.4",
    "ionicons": "^4.6.2",
    "web-social-share": "^6.2.0"
  },
  "keywords": [
    "web components",
    "presentation",
    "slides",
    "slideshow",
    "talk",
    "elemental",
    "customizable user interface",
    "dynamic components",
    "angular"
  ]
}
